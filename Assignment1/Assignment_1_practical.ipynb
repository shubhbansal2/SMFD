        "    n = int(input())\n",
        "    dp = [0] * (n + 1)\n",
        "    dp[0] = 1\n",
        "    for i in range(1, n + 1):\n",
        "        for j in range(1, i+1):\n",
        "            dp[j] += dp[j - 1]\n",
        "            dp[j]%=1e9+7\n",
        "    print(dp[n])"
      ]
    }
